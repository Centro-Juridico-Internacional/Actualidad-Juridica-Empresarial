{
    "headers": [
        {
            "source": "/_astro/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/fonts/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).(png|jpg|jpeg|svg|webp|ico|gif|avif|tiff|bmp|mp4|webm|avi|mov|mkv|mp3|wav|ogg|m4a|pdf|doc|docx|xls|xlsx|ppt|pptx|txt|csv|json|xml|zip|rar)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ],
    "rewrites": []
}